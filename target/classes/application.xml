<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:cutesource="http://hz.yk/spring/schema/people"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://hz.yk/spring/schema/people http://hz.yk/spring/schema/people.xsd">
    <!--
    xmlns 规定了默认命名空间的声明
    xmlns:xsi 可用的 XML Schema 实例命名空间
    xsi:schemaLocation 命名空间使用的 XML schema 的位置
    -->
    <import resource="beans.xml" />
    <cutesource:people id="cutesource" name="yk" age="27"/>
</beans>